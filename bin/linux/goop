#!/bin/bash
data=""
while read line; do
    new=$(echo "$line" | python3 -c "import urllib.parse, sys; print(urllib.parse.quote(sys.stdin.read()) + '\\n')")
    data="$data$new"
done
if [ -z "$data" ]; then
    echo "no data"
    exit 1
fi
curl 'http://center.voidedtech.com/goop/save/' \
      -X POST -H 'User-Agent: cli/bash' \
      --compressed \
      -s \
      -H 'Content-Type: application/x-www-form-urlencoded' \
      --data-raw "content=$data&expire=on" > /dev/null
