_git_oclone() {
    local cur opts
    if [ "$COMP_CWORD" -eq 2 ]; then
        cur=${COMP_WORDS[COMP_CWORD]}
        opts=$(ls $HOME/.env/workstations/git | sed 's/$/:/g')
        COMPREPLY=( $(compgen -W "$opts" -- "$cur") )
    fi
}

_apod() {
    local cur opts
    if [ "$COMP_CWORD" -eq 1 ]; then
        cur=${COMP_WORDS[COMP_CWORD]}
        opts=$(ls /opt/repositories)
        COMPREPLY=( $(compgen -W "$opts" -- "$cur") )
    fi
}

complete -F _apod -o bashdefault -o default apod
