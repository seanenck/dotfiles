# tdiff completion

_tdiff() {
  local cur opts
  cur=${COMP_WORDS[COMP_CWORD]}
  opts="${opts}--exclude "
  opts="${opts}--exit-early "
  opts="${opts}--headers-only "
  opts="${opts}--quiet "
  opts="${opts}--short "
  COMPREPLY=($(compgen -W "$opts" -- "$cur"))
}

complete -F _tdiff -o default -f -o bashdefault tdiff
