{
  "Flags": [
    "update-system",
    "linux"
  ],
  "Settings": {
    "State": ".local/state/remotes",
    "Sources": {
      "https://github.com/rclone/rclone": "Git",
      "https://github.com/golang/go": "Git"
    },
    "Modes": {
      "Alpine": {
        "Command": "apk",
        "Arguments": ["info"],
        "Filter": "^linux-virt-([a-zA-Z0-9.-]*?) description:"
      },
      "Git": {
        "Command": "git",
        "Arguments": ["ls-remote", "--tags"],
        "Filter": "[a-f0-9]+\trefs/tags/([a-zA-Z0-9._-]+?)$"
      }
    }
  }
}
