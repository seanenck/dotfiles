{
  "boot": {
    "efi": {
      "store": "$CONFIG/efi"
    }
  },
  "identifier": "$CONFIG/machine",
  "resources": {
    "cpus": 2,
    "memory": 8192
  },
  "serial": "none",
  "disks": [
    {
      "mode": "block",
      "path": "$CONFIG/boot.iso"
    },
    {
      "mode": "nvme",
      "path": "$CONFIG/apkovl.img"
    },
    {
      "mode": "nvme",
      "path": "$CONFIG/var.img"
    },
    {
      "mode": "nvme",
      "path": "$CONFIG/home.img"
    }
  ],
  "env": {
    "CONFIG": "~/Linux/VMs/cusp"
  },
  "shares": {
    "~/Active": {},
    "~/Linux": {},
    "~/Env": {},
    "~/Workspace": {},
    "~/Downloads": {},
    "~/Documents": {}
  },
  "networks": [
    {
      "mac": "5e:49:6b:f7:7f:84",
      "mode": "nat"
    }
  ]
}
