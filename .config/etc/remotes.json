{
  "State": ".local/state/remotes",
  "Sources": {
    "https://github.com/kovidgoyal/kitty": "Git",
    "https://github.com/rxhanson/rectangle": "Git"  
  },
  "Modes": {
    "Git": {
      "Command": ["git", "ls-remote", "--tags"],
      "Filter": [
        "[a-zA-Z0-9.]+$",
        "[0-9].[0-9]"
      ]
    },
    "Brew": {
      "Command": ["brew", "info", "--json"],
      "Field": "versions.stable"
    }
  }
}
