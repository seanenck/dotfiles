#!/usr/bin/env bash
export PATH="/opt/homebrew/bin:$HOME/.local/bin:$PATH"
for f in coreutils findutils make gnu-sed; do
  export PATH="/opt/homebrew/opt/$f/libexec/gnubin:$PATH"
done
export EXCLUDE_FILES='-not -name .DS_Store -not -name .localized'
export HOME_GIT="$HOME/Git"
export TASK_CACHE="$HOME/Library/voidedtech"
export SYSIM="$TASK_CACHE/sysim"
export TASK_SOURCE="$HOME_GIT/tasks"
export GIT_SOURCES="$HOME/Active/git"
export EXTERNAL_CACHE="$HOME/Active/externals"
