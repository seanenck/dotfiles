[include]
    path = ~/.gitconfig_local
[core]
    autocrlf = input
[grep]
    lineNumber = true
[log]
    date = iso-strict
[pull]
    rebase = false
[push]
    default = simple
[status]
    short = true
    branch = true
    showStash = true
    showUntrackedFiles = all
[user]
    name = Sean Enck
    email = enckse@voidedtech.com
[help]
    format = man
[merge]
    conflictstyle = diff3
