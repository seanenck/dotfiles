[include]
    path = ~/.config/git/config
[core]
    autocrlf = input
[grep]
    lineNumber = true
[log]
    date = iso-strict
[pull]
    rebase = false
[push]
    default = simple
[status]
    short = true
    branch = true
    showStash = true
    showUntrackedFiles = all
[user]
    name = Sean Enck
    useConfigOnly = true
[help]
    format = man
[merge]
    conflictstyle = diff3
[url "store.voidedtech.com:/mnt/storage/active/git/private/"]
   insteadOf = private:
[includeIf "hasconfig:remote.*.url:private:*"]
   path = "~/.config/git/user"
