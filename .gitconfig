[include]
    path = ~/.config/git/config
[core]
    autocrlf = input
[grep]
    lineNumber = true
[log]
    date = iso-strict
[pull]
    rebase = false
[push]
    default = simple
[status]
    short = true
    branch = true
    showStash = true
    showUntrackedFiles = all
[user]
    name = Sean Enck
[help]
    format = man
[merge]
    conflictstyle = diff3
[url "router.voidedtech.com:/mnt/storage/active/git/public/"]
   insteadOf = upstream:
[url "router.voidedtech.com:/mnt/storage/active/git/private/"]
   insteadOf = private:
