export DOTFILES="$HOME/.env/dotfiles"
export LOCAL_FS="$HOME/.fs"
export MACHINE_TYPE=$(uname -s | tr '[:upper:]' '[:lower:]')
export PATH="$DOTFILES/bin:$DOTFILES/bin/$MACHINE_TYPE:$LOCAL_FS/usr/local/bin:$PATH"
export LOCKBOX_STORE="$HOME/.env/passwords"
export LOCKBOX_KEYMODE="command"
export LOCKBOX_TOTP="keys/totp/"
source $HOME/.env/workstations/bashrc
source $HOME/.env/workstations/bashrc.$MACHINE_TYPE
