#!/usr/bin/env bash
_git_oclone() {
    local cur opts
    if [ "$COMP_CWORD" -eq 2 ]; then
        cur=${COMP_WORDS[COMP_CWORD]}
        opts=$(git oclone --list)
        COMPREPLY=( $(compgen -W "$opts" -- "$cur") )
    fi
}

_new-toolbox() {
    local cur opts
    if [ "$COMP_CWORD" -eq 1 ]; then
        cur=${COMP_WORDS[COMP_CWORD]}
        opts=$(new-toolbox --list)
        COMPREPLY=( $(compgen -W "$opts" -- "$cur") )
    fi
}

complete -F _new-toolbox -o bashdefault -o default new-toolbox

